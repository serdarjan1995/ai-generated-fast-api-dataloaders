{
    "openapi": "3.0.2",
    "info": {
        "title": "Milvus Loader",
        "version": "0.1.0"
    },
    "paths": {
        "/load_data/": {
            "post": {
                "tags": [
                    "Milvus Loader"
                ],
                "summary": "Load Data Endpoint",
                "operationId": "load_data_endpoint_load_data__post",
                "requestBody": {
                    "content": {
                        "application/json": {
                            "schema": {
                                "$ref": "#/components/schemas/RequestModel"
                            }
                        }
                    },
                    "required": true
                },
                "responses": {
                    "200": {
                        "description": "Successful Response",
                        "content": {
                            "application/json": {
                                "schema": {}
                            }
                        }
                    },
                    "422": {
                        "description": "Validation Error",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/HTTPValidationError"
                                }
                            }
                        }
                    }
                }
            }
        }
    },
    "components": {
        "schemas": {
            "HTTPValidationError": {
                "title": "HTTPValidationError",
                "type": "object",
                "properties": {
                    "detail": {
                        "title": "Detail",
                        "type": "array",
                        "items": {
                            "$ref": "#/components/schemas/ValidationError"
                        }
                    }
                }
            },
            "QueryModel": {
                "title": "QueryModel",
                "required": [
                    "query_vector",
                    "collection_name"
                ],
                "type": "object",
                "properties": {
                    "query_vector": {
                        "title": "Query Vector",
                        "type": "array",
                        "items": {
                            "type": "number"
                        }
                    },
                    "collection_name": {
                        "title": "Collection Name",
                        "type": "string"
                    },
                    "limit": {
                        "title": "Limit",
                        "type": "integer",
                        "default": 10
                    }
                }
            },
            "RequestModel": {
                "title": "RequestModel",
                "required": [
                    "host",
                    "user",
                    "password",
                    "query"
                ],
                "type": "object",
                "properties": {
                    "host": {
                        "title": "Host",
                        "type": "string"
                    },
                    "port": {
                        "title": "Port",
                        "type": "integer",
                        "default": 19530
                    },
                    "user": {
                        "title": "User",
                        "type": "string"
                    },
                    "password": {
                        "title": "Password",
                        "type": "string"
                    },
                    "use_secure": {
                        "title": "Use Secure",
                        "type": "boolean",
                        "default": false
                    },
                    "query": {
                        "$ref": "#/components/schemas/QueryModel"
                    }
                }
            },
            "ValidationError": {
                "title": "ValidationError",
                "required": [
                    "loc",
                    "msg",
                    "type"
                ],
                "type": "object",
                "properties": {
                    "loc": {
                        "title": "Location",
                        "type": "array",
                        "items": {
                            "anyOf": [
                                {
                                    "type": "string"
                                },
                                {
                                    "type": "integer"
                                }
                            ]
                        }
                    },
                    "msg": {
                        "title": "Message",
                        "type": "string"
                    },
                    "type": {
                        "title": "Error Type",
                        "type": "string"
                    }
                }
            }
        }
    }
}