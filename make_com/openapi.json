{
    "openapi": "3.0.2",
    "info": {
        "title": "FastAPI",
        "version": "0.1.0"
    },
    "paths": {
        "/pass-response-to-webhook/": {
            "post": {
                "summary": "Pass response to Make.com webhook",
                "description": "Pass a response object to a specified Make.com webhook url.",
                "operationId": "pass_response_to_webhook_pass_response_to_webhook__post",
                "requestBody": {
                    "content": {
                        "application/json": {
                            "schema": {
                                "$ref": "#/components/schemas/RequestModel"
                            }
                        }
                    },
                    "required": true
                },
                "responses": {
                    "200": {
                        "description": "Successful Response",
                        "content": {
                            "application/json": {
                                "schema": {}
                            }
                        }
                    },
                    "422": {
                        "description": "Validation Error",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/HTTPValidationError"
                                }
                            }
                        }
                    }
                }
            }
        }
    },
    "components": {
        "schemas": {
            "HTTPValidationError": {
                "title": "HTTPValidationError",
                "type": "object",
                "properties": {
                    "detail": {
                        "title": "Detail",
                        "type": "array",
                        "items": {
                            "$ref": "#/components/schemas/ValidationError"
                        }
                    }
                }
            },
            "NodeWithScoreModel": {
                "title": "NodeWithScoreModel",
                "required": [
                    "node"
                ],
                "type": "object",
                "properties": {
                    "node": {
                        "title": "Node",
                        "type": "object"
                    },
                    "score": {
                        "title": "Score",
                        "type": "number"
                    }
                }
            },
            "RequestModel": {
                "title": "RequestModel",
                "required": [
                    "webhook_url"
                ],
                "type": "object",
                "properties": {
                    "webhook_url": {
                        "title": "Webhook Url",
                        "type": "string"
                    },
                    "response": {
                        "$ref": "#/components/schemas/ResponseModel"
                    }
                }
            },
            "ResponseModel": {
                "title": "ResponseModel",
                "required": [
                    "response",
                    "source_nodes"
                ],
                "type": "object",
                "properties": {
                    "response": {
                        "title": "Response",
                        "type": "string"
                    },
                    "source_nodes": {
                        "title": "Source Nodes",
                        "type": "array",
                        "items": {
                            "$ref": "#/components/schemas/NodeWithScoreModel"
                        }
                    },
                    "query": {
                        "title": "Query",
                        "type": "string"
                    }
                }
            },
            "ValidationError": {
                "title": "ValidationError",
                "required": [
                    "loc",
                    "msg",
                    "type"
                ],
                "type": "object",
                "properties": {
                    "loc": {
                        "title": "Location",
                        "type": "array",
                        "items": {
                            "anyOf": [
                                {
                                    "type": "string"
                                },
                                {
                                    "type": "integer"
                                }
                            ]
                        }
                    },
                    "msg": {
                        "title": "Message",
                        "type": "string"
                    },
                    "type": {
                        "title": "Error Type",
                        "type": "string"
                    }
                }
            }
        }
    }
}